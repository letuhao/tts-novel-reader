{
  "name": "english-tutor-backend",
  "version": "0.1.0",
  "description": "English Tutor Backend - AI-powered English learning platform",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc && npm run copy-migrations",
    "copy-migrations": "node scripts/copy-migrations.js",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "dev:nodemon": "nodemon --exec tsx src/server.ts",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "english",
    "tutor",
    "ai",
    "education",
    "learning"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/compression": "^1.7.5",
    "@types/pg": "^8.11.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "axios": "^1.7.9",
    "compression": "^1.7.4",
    "pg": "^8.13.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "uuid": "^11.0.3",
    "zod": "^3.23.8",
    "multer": "^1.4.5-lts.1",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.18.0",
    "nodemon": "^3.1.9",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

